<Window x:Class="AudioPlayerWPF.RenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AudioPlayerWPF"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Background="White"
        ShowInTaskbar="False"
        Height="140" Width="256">
    <Grid>
        <DockPanel>
            <StackPanel Orientation="Horizontal" DockPanel.Dock="Bottom" HorizontalAlignment="Right">
                <Button Width="75" Height="23" Content="Cancel" Click="CancelButton_Click" Margin="0,0,6,6" />
                <Button Width="75" Height="23" Content="Confirm" Name="confirmButton" Margin="6,0,6,6" />
            </StackPanel>
            <GroupBox Name="groupBox" Header="Rename &quot;&quot;" Margin="6,3,6,6" Padding="6" VerticalAlignment="Top">
                <TextBox Name="textBox" Height="23" MaxLength="32" VerticalContentAlignment="Center">

                </TextBox>
            </GroupBox>
        </DockPanel>
    </Grid>
</Window>
